CLAUDE_API_KEY_2=your_claude_api_key_2_here

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# Google Sheets Configuration
GOOGLE_SHEETS_CREDENTIALS_FILE=path/to/your/google-credentials.json
